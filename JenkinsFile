stage 'Checkout_JenkinsWorkFlow'
node {
git 'https://github.com/exorcist007/JenkinsWorkflowBuild.git'
stage 'build'
}
stage 'clean_JenkinsWorkflow'
node{
bat 'gradle clean'
}
stage 'build_JenkinsWorkflow'
node{
 bat 'gradle build --info'   
    
}

stage 'Checkout_TF-IDF-Casc'
